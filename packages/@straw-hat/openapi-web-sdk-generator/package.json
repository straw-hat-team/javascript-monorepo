{
  "name": "@straw-hat/openapi-web-sdk-generator",
  "version": "2.12.0",
  "description": "A Web SDK Generator using OpenAPI v3.",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/nodejs-monorepo.git",
    "directory": "packages/@straw-hat/openapi-web-sdk-generator"
  },
  "license": "MIT",
  "keywords": [
    "openapi",
    "generator",
    "fetch",
    "http",
    "sdk",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build && yarn oclif:manifest && yarn oclif:readme",
    "postpack": "yarn run -T rimraf oclif.manifest.json",
    "docs:typedoc": "yarn run -T typedoc",
    "oclif:manifest": "oclif-dev manifest",
    "oclif:readme": "oclif-dev readme --multi --dir=docs/commands"
  },
  "bin": {
    "sht-openapi-web-sdk-generator": "bin/sht-openapi-web-sdk-generator"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "README.md",
    "templates",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@oclif/command": "^1.8.16",
    "@oclif/errors": "^1.3.5",
    "@oclif/help": "^1.0.1",
    "@oclif/plugin-help": "^5.1.12",
    "@stoplight/json-ref-resolver": "^3.1.3",
    "@straw-hat/fancy-map": "^2.0.0",
    "@straw-hat/prettier-config": "^2.0.2",
    "change-case": "^4.1.2",
    "cosmiconfig": "^7.0.1",
    "debug": "^4.3.4",
    "del": "^6.1.1",
    "import-from": "^4.0.0",
    "make-dir": "^3.1.0",
    "mustache": "^4.2.0",
    "openapi-types": "^11.0.1",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@straw-hat/tsconfig": "workspace:*",
    "@straw-hat/types": "^0.5.0",
    "@swc/core": "^1.2.242",
    "@swc/jest": "^0.2.21",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.36",
    "@types/prettier": "^2.6.3",
    "jest": "^28.1.0",
    "typescript": "^4.7.2"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "sht-openapi-web-sdk-generator",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}

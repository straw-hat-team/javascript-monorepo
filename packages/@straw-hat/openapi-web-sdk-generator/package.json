{
  "name": "@straw-hat/openapi-web-sdk-generator",
  "version": "2.12.0",
  "description": "A Web SDK Generator using OpenAPI v3.",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/nodejs-monorepo.git",
    "directory": "packages/@straw-hat/openapi-web-sdk-generator"
  },
  "license": "MIT",
  "keywords": [
    "openapi",
    "generator",
    "fetch",
    "http",
    "sdk",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build && yarn oclif:manifest && yarn oclif:readme",
    "postpack": "yarn run -T rimraf oclif.manifest.json",
    "docs:typedoc": "yarn run -T typedoc",
    "oclif:manifest": "oclif-dev manifest",
    "oclif:readme": "oclif-dev readme --multi --dir=docs/commands"
  },
  "bin": {
    "sht-openapi-web-sdk-generator": "bin/sht-openapi-web-sdk-generator"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "README.md",
    "templates",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@oclif/command": "^1.8.36",
    "@oclif/errors": "^1.3.6",
    "@oclif/help": "^1.0.15",
    "@oclif/plugin-help": "^5.2.19",
    "@stoplight/json-ref-resolver": "^3.1.6",
    "@straw-hat/fancy-map": "^3.0.1",
    "@straw-hat/prettier-config": "^3.1.2",
    "change-case": "^4.1.2",
    "cosmiconfig": "^8.3.6",
    "debug": "^4.3.4",
    "del": "^7.1.0",
    "import-from": "^4.0.0",
    "make-dir": "^4.0.0",
    "mustache": "^4.2.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@straw-hat/tsconfig": "workspace:*",
    "@straw-hat/types": "^0.5.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@types/debug": "^4.1.9",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.6",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.18.0",
    "@types/prettier": "^3.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "sht-openapi-web-sdk-generator",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}

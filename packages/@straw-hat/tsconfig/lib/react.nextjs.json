{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base",
  "display": "React Next.js",
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "emitDeclarationOnly": false,
    "isolatedModules": true,
    "incremental": true,
    "moduleResolution": "bundler",
    "declaration": false,
    "declarationMap": false,
    "plugins": [{ "name": "next" }]
  }
}

{
  "name": "@straw-hat/next-sdk",
  "version": "0.5.1",
  "description": "A Software Development Kit for NextJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/nodejs-monorepo.git",
    "directory": "packages/@straw-hat/next-sdk"
  },
  "license": "MIT",
  "keywords": [
    "straw-hat-team",
    "nextjs",
    "next",
    "react",
    "sdk"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "yarn prebuild && yarn run -T tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build",
    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "next": "^12.0.4",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@straw-hat/types": "^0.5.0"
  },
  "devDependencies": {
    "@straw-hat/tsconfig": "workspace:*",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.18.0",
    "@types/react": "^18.2.23",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^13.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  }
}

{
  "name": "@straw-hat/fancy-map",
  "version": "3.0.1",
  "description": "An Extended Native Map",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript-monorepo.git",
    "directory": "packages/fancy-map"
  },
  "license": "MIT",
  "keywords": [
    "map",
    "cache",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",

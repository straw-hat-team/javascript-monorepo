    "test:jest": "yarn run -T jest",
    "prepack": "yarn build",
    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@straw-hat/tsconfig": "^3.0.0",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
